        "import pandas as pd\n",
        "import numpy as np\n",
        "import datetime\n",
        "\n",
        "def convert_df_to_snowflake_safe(df: pd.DataFrame) -> pd.DataFrame:\n",
        "    \"\"\"\n",
        "    Convert all columns in a Pandas DataFrame to native Python types\n",
        "    so it is safe for Snowflake UDF or save_as_table.\n",
        "    \"\"\"\n",
        "    def _to_python_scalar(x):\n",
        "        if x is None or pd.isna(x):\n",
        "            return None\n",
        "        # Integers\n",
        "        if isinstance(x, (np.integer, np.int64, np.int32)):\n",
        "            return int(x)\n",
        "        # Floats\n",
        "        if isinstance(x, (np.floating, np.float64, np.float32)):\n",
        "            return float(x)\n",
        "        # Boolean\n",
        "        if isinstance(x, np.bool_):\n",
        "            return bool(x)\n",
        "        # Timestamp\n",
        "        if isinstance(x, pd.Timestamp):\n",
        "            return x.to_pydatetime()\n",
        "        # Native Python types\n",
        "        if isinstance(x, (int, float, str, bool, datetime.date, datetime.datetime)):\n",
        "            return x\n",
        "        # Fallback to string\n",
        "        return str(x)\n",
        "\n",
        "    for col in df.columns:\n",
        "        df[col] = df[col].apply(_to_python_scalar)\n",
        "\n",
        "    return df\n"
      ],
      "execution_count": null,
      "outputs": []
    },
